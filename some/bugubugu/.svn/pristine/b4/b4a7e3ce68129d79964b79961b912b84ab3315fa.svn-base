<ion-view view-title="我的">
  <ion-nav-buttons side="right">
    <button ui-sref="tab.account.shezhi" class="button button-clear button-outline" >
      设置</button>
  </ion-nav-buttons>
  <ion-content class="zx-my-content" has-bouncing="true">
    <div class="zx-my-bj">
      <div class="zx-my-pic">
        <img src="img/mypic/Path2Copy11.png" alt="">
      </div>
      <span>昵称</span>
      <span class="zx-my-bianji">编辑</span>
      <img class="zx-my-touxiang" src="img/mypic/Path2Copy.png" alt="">
      <div ui-sref="tab.account.personal" class="zx-my-right">
        <span>个人主页</span>
        <img src="img/mypic/Rectangle1306Copy4.png" alt="">
      </div>
    </div>
    <div ui-sref="tab.account.attention" class="zx-my-guanzhu">
      <span>关注123人</span>
      <span>&nbsp|&nbsp</span>
      <span>被关注23人</span>
      <div>查看全部</div>
    </div>
    <div ui-sref="tab.account.remind" class="zx-my-list">
      <img style="margin-top:0.2rem;" src="img/mypic/Group2.png" alt="">
      <span>提醒</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
      <div>2条未读</div>
    </div>
    <div ui-sref="tab.account.qiutuishu" class="zx-my-list">
      <img src="img/mypic/Group6.png" alt="">
      <span style="margin-top:0.1rem">我的求书&推荐</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
    </div>
    <div ui-sref="tab.account.book" class="zx-my-list">
      <img src="img/mypic/Group5.png" alt="">
      <span>我的图书</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
    </div>
    <div ui-sref="tab.account.jifen" class="zx-my-list">
      <img src="img/mypic/Group4.png" alt="">
      <span>我的积分</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
    </div>
    <div ui-sref="tab.account.shoucang" class="zx-my-list">
      <img style="margin-top:0.2rem;" src="img/mypic/Group3.png" alt="">
      <span>收藏</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
      <div>
        <span>1站点</span>
        <span>|</span>
        <span>1本书</span>
      </div>
    </div>
  </ion-content>
</ion-view>
