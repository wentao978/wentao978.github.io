<ion-view view-title="我的图书" hide-tabs>
  <ion-content class="zx-my-content zx-book-content" has-bouncing="true">
    <div ui-sref="tab.account.jieyue" class="zx-my-list">
      <span>我的借阅</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
      <div class="zx-book-weihuan">{{book}}</div>
    </div>
    <div ui-sref="tab.account.daitoupiao" class="zx-my-list">
      <span>待投漂图书</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
      <div>3本</div>
    </div>
    <div ui-sref="tab.account.yitoupiao" class="zx-my-list">
      <span>已投票图书</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
      <div>3本</div>
    </div>
    <div class="zx-my-list">
      <span>帮他人投漂图书</span>
      <img class="zx-my-rightjiantou" src="img/mypic/Rectangle1306Copy4.png" alt="">
      <div>3本</div>
    </div>
  </ion-content>
</ion-view>